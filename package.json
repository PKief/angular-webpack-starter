    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.9",
    "core-js": "^2.5.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.8",
    "typescript": "^2.8.1",
    "zone.js": "^0.8.25"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/core-js": "^0.9.46",
