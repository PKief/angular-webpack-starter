{
  "name": "angular-webpack-starter",
  "version": "1.0.0",
  "description": "A webpack starter for angular web apps",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --color",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.ts --progress --profile --bail && npm run build-server",
    "build-server": "node -r ts-node/register server.ts",
    "tslint": "tslint ./src/**/*.ts -t stylish"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.5.3",
    "express": "^4.16.2",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine-html-reporter": "^0.2.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6",
    "typescript": "^2.6.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/core-js": "^0.9.44",
    "@types/express": "^4.11.0",
    "@types/jasmine": "^2.8.3",
    "@types/lodash": "^4.14.92",
    "@types/node": "^9.3.0",
    "@types/webpack": "^3.8.2",
    "angular-router-loader": "^0.8.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "lite-server": "^2.3.0",
