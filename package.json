{
  "name": "angular-webpack-starter",
  "version": "1.0.0",
  "description": "A webpack starter for angular web apps",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --color",
    "test": "karma start",
    "prebuild": "rimraf dist && npm run lint",
    "build": "webpack --config config/webpack.prod.ts --progress --profile --bail",
    "postbuild": "ts-node server.ts",
    "lint": "tslint ./src/**/*.ts -t stylish"
