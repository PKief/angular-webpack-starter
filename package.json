    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.0",
    "tslint-loader": "^3.5.3",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.10.1",
    "webpack-merge": "^4.1.1"
  }
